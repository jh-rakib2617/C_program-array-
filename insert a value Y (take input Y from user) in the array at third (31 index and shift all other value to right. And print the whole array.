#include<stdio.h>
int main()
{
    int arr[100]={5,2,3,4,1,9,10,8,7,6};
    int i,y;
    printf("Enter value Y:  ");
    scanf("%d",&y);

    for(i = 99; i > 3; i--)
    {
        arr[i]=arr[i-1];
    }
     arr[3] = y;
    for(i = 0; i< 100; i++)
    {
        printf("%d\n",arr[i]);
    }
    return 0;
}
